<launch>
  <master auto="start"/>

    <!-- Run move_base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find rover_teleop)/config/costmap_common_params.yaml" command="load"/>
    <rosparam file="$(find rover_teleop)/config/local_costmap_params.yaml" command="load"/>
    <rosparam file="$(find rover_teleop)/config/global_costmap_params.yaml" command="load"/>
    <rosparam file="$(find rover_teleop)/config/base_local_planner_params.yaml" command="load"/>

    <param name="base_global_planner" value="carrot_planner/CarrotPlanner"/>
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS"/>
    <param name="global_costmap/obstacle_layer" value="costmap_2d::ObstacleLayer"/>
    <param name="local_costmap/obstacle_layer" value="costmap_2d::ObstacleLayer"/>
  </node>
</launch>
